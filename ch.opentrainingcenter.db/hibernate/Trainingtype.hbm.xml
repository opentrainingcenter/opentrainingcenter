<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="ch.opentrainingcenter.transfer.impl.Trainingtype"
		table="TRAININGTYPE" schema="PUBLIC">
		<id name="id" type="int">
			<column name="ID" />
			<generator class="assigned" />
		</id>
		<property name="title" type="string">
			<column name="TITLE" />
		</property>
		<property name="description" type="string">
			<column name="DESCRIPTION" />
		</property>
		<property name="imageicon" type="string">
			<column name="IMAGEICON" />
		</property>
		<!-- <set name="trainings" table="TRAINING" inverse="true" lazy="true" 
			fetch="select"> <key> <column name="ID_FK_TRAININGTYPE" /> </key> <one-to-many 
			class="ch.opentrainingcenter.transfer.impl.Training" /> </set> -->
	</class>
</hibernate-mapping>
